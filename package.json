{
  "name": "nova26",
  "version": "1.0.0",
  "description": "AI-powered IDE that uses 21 specialized agents to build software through orchestrated task execution",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:mock": "tsx src/test/mock-run.ts",
    "test:integration": "tsx src/test/integration-test.ts",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test:changed": "vitest run --changed",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.test.ts": "vitest run"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "llm",
    "code-generation"
  ],
  "author": "Matrix Agent",
  "license": "MIT",
  "dependencies": {
    "@fal-ai/client": "^1.9.1",
    "@instructor-ai/instructor": "^1.7.0",
    "@tremor/react": "^3.18.7",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.69.3",
    "convex": "^1.31.7",
    "fast-check": "^4.5.3",
    "framer-motion": "^11.0.0",
    "ioredis": "^5.9.3",
    "langfuse": "^3.38.6",
    "ollama": "^0.5.0",
    "openai": "^6.22.0",
    "promptfoo": "^0.120.19",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.7.0",
    "ts-morph": "^27.0.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.11.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
